<table id="margintable" class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Titulo</th>
      <th>Articulo</th>
       <th>category</th>
      <th>Like</th>
    

      <th colspan="7"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id%></td>
        <td><%= post.titulo %></td>
        <td><%= post.articulo %></td>
        <td><%= post.category %></td>
        
        <td><%= post.like %></td>
        <td><a href="/posts/like/<%= post.id%>">like</a></td>
        <td><a href="/posts/dislike/<%= post.id%>"> dislike</a></td>
        
      </tr>



    <% end %>

  </tbody>
</table>


